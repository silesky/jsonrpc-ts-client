{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "dts test",
    "test:debug": "DEBUG=jsonrpc-ts-client npm test",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "jsonrpc-ts-client",
  "author": "Seth Silesky",
  "module": "dist/jsonrpc-ts-client.esm.js",
  "size-limit": [
    {
      "path": "dist/jsonrpc-ts-client.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/jsonrpc-ts-client.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "debug": "^4.3.3"
  },
  "peerDependencies": {
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.4",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.0.3",
    "@types/uuid": "^8.3.3",
    "dts-cli": "^1.1.3",
    "husky": "^7.0.4",
    "msw": "^0.36.3",
    "prettier": "^2.5.1",
    "size-limit": "^7.0.4",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "uuid": "^8.3.2"
  }
}